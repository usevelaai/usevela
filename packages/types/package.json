{
	"name": "@vela/types",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "tsup src/index.ts --format esm --dts",
		"dev": "tsup src/index.ts --format esm --dts --watch",
		"type-check": "tsc --noEmit",
		"lint": "biome check src",
		"lint:fix": "biome check --write src"
	},
	"devDependencies": {
		"tsup": "^8.3.5",
		"typescript": "^5.7.2"
	}
}
